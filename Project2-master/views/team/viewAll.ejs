<%- include("../includes/header.ejs", {extra: '', title:"Teams - View All", url:"http://assets2.ignimgs.com/2013/11/13/marvel-logo-1280jpg-8851ea_1280w.jpg"}); %>
<div class="minytbox">
    <a href="/"><h4><< back</h4></a>
</div>
<rj>
    <h1>Teams</h1>
    <mycontainer>
        <% for(var i=0; result.length > i; i++ ) { %>
        <div class="transimgXL">
            <a href="/team/?team_id=<%= result[i].team_id %>">
                <img src="<%= result[i].team_image_url %>" alt="<%= result[i].team_name %>">
                <p><%= result[i].team_name %></p>
            </a>
        </div>
        <% } %>
    </mycontainer>
</rj>
</body>
</html>